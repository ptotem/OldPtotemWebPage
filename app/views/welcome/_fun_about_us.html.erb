<script type="text/javascript">

    $(document).ready(init);

    function init() {
        /* ========== DRAWING THE PATH AND INITIATING THE PLUGIN ============= */

        $.fn.scrollPath("getPath")
            // Move to 'start' element
                .moveTo(400, 50, {name:"description"})
            // Line to 'description' element
                .lineTo(400, 400, {name:"description1"})
                .lineTo(400, 800, {name:"description2"})
            // Arc down and line to 'syntax'
                .arc(200, 1200, 400, -Math.PI / 2, Math.PI / 1, true, {name:"description3"})
                .arc(200, 1200, 400, Math.PI, Math.PI / 2, true , {name:"description4"})
                .lineTo(800, 1600, {name:"description5"})
            // Continue line to 'scrollbar'
                .lineTo(1750, 1600, {name:"description6"})
            // Arc up while rotating
                .arc(1800, 1000, 600, Math.PI / 2, 0, true, {rotate:Math.PI / 2 }, {name:"description7"})
            // Line to 'rotations'
                .lineTo(2400, 750)
            // Rotate in place
                .rotate(3 * Math.PI / 2, {
                    name:"description8"
                })
            // Continue upwards to 'source'
                .lineTo(2400, -200, {
                    name:"description10"
                })
                .lineTo(2400, -700)
            // Small arc downwards
                .arc(2250, -700, 150, 0, -Math.PI / 2, true, {
                    name:"description11"
                })

            //Line to 'follow'
                .lineTo(1350, -850, {
                    name:"description12"
                })
            // Arc and rotate back to the beginning.
                .arc(1300, 50, 900, -Math.PI / 2, -Math.PI / 1.5, true, {rotate:Math.PI * 1.75, name:"description13"})
                .arc(1300, 50, 900, -Math.PI / 1.5, -Math.PI, true, {rotate:Math.PI * 2, name:"description"});

        // We're done with the path, let's initate the plugin on our wrapper element
        $(".wrapper").scrollPath({drawPath:true, wrapAround:true, scrollBar:false});

        /* ===================================================================== */

        $(".settings .show-path").click(
                function (e) {
                    e.preventDefault();
                }).toggle(function () {
                    $(this).text("Hide Path");
                }, function () {
                    $(this).text("Show Path");
                });
    }


    function highlight(element) {
        if (!element.hasClass("highlight")) {
            element.addClass("highlight");
            setTimeout(function () {
                element.removeClass("highlight");
            }, 2000);
        }
    }
    function ordinal(num) {
        return num + (
                (num % 10 == 1 && num % 100 != 11) ? 'st' :
                        (num % 10 == 2 && num % 100 != 12) ? 'nd' :
                                (num % 10 == 3 && num % 100 != 13) ? 'rd' : 'th'
                );
    }


</script>